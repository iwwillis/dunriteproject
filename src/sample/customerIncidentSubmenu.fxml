<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.customerIncidentSubmenuController">
   <children>
      <Button fx:id="customerIncidentButton" layoutX="150.0" layoutY="112.0" mnemonicParsing="false" onAction="#handleCustomerIncidentButton" prefHeight="69.0" prefWidth="143.0" text="Customer Incident">
         <styleClass>
            <String fx:value="tealbg" />
            <String fx:value="whitetxt" />
         </styleClass></Button>
      <Button fx:id="backButton" layoutX="452.0" layoutY="341.0" mnemonicParsing="false" onAction="#handleBackButton" text="Back">
         <styleClass>
            <String fx:value="redbg" />
            <String fx:value="whitetxt" />
         </styleClass></Button>
      <Button fx:id="customerIncidentTypeButton" layoutX="300.0" layoutY="112.0" mnemonicParsing="false" onAction="#handleCustomerIncidentTypeButton" prefHeight="69.0" prefWidth="143.0" text="Customer Incident Type">
         <styleClass>
            <String fx:value="tealbg" />
            <String fx:value="whitetxt" />
         </styleClass></Button>
      <Label layoutX="228.0" layoutY="36.0" styleClass="tealtxt" text="Manage Customer Incident" />
      <Button fx:id="customerIncidentStatusButton" layoutX="150.0" layoutY="191.0" mnemonicParsing="false" onAction="#handleCustomerIncidentStatusButton" prefHeight="69.0" prefWidth="143.0" text="Customer Incident Status" textAlignment="CENTER" wrapText="true">
         <styleClass>
            <String fx:value="tealbg" />
            <String fx:value="whitetxt" />
         </styleClass></Button>
      <Button fx:id="customerEmployeeIncidentButton" layoutX="300.0" layoutY="191.0" mnemonicParsing="false" onAction="#handleCustomerEmployeeIncidentButton" prefHeight="69.0" prefWidth="143.0" text="Customer Employee Incident" textAlignment="CENTER" wrapText="true">
         <styleClass>
            <String fx:value="tealbg" />
            <String fx:value="whitetxt" />
         </styleClass></Button>
      <Button fx:id="homeButton" layoutX="507.0" layoutY="341.0" mnemonicParsing="false" onAction="#handleHomeButton" text="Home">
         <styleClass>
            <String fx:value="redbg" />
            <String fx:value="whitetxt" />
         </styleClass></Button>
   </children>
</AnchorPane>
