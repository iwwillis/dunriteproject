<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.employeeIncidentSubmenuController">
   <children>
      <Button fx:id="employeeIncidentButton" layoutX="150.0" layoutY="112.0" mnemonicParsing="false" onAction="#handleEmployeeIncidentButton" prefHeight="69.0" prefWidth="143.0" text="Employee Incident">
         <styleClass>
            <String fx:value="tealbg" />
            <String fx:value="whitetxt" />
         </styleClass></Button>
      <Button fx:id="backButton" layoutX="452.0" layoutY="341.0" mnemonicParsing="false" onAction="#handleBackButton" text="Back">
         <styleClass>
            <String fx:value="whitebg" />
            <String fx:value="redtxt" />
         </styleClass></Button>
      <Button fx:id="employeeIncidentTypeButton" layoutX="300.0" layoutY="112.0" mnemonicParsing="false" onAction="#handleEmployeeIncidentTypeButton" prefHeight="69.0" prefWidth="143.0" text="Employee Incident Type">
         <styleClass>
            <String fx:value="tealbg" />
            <String fx:value="whitetxt" />
         </styleClass></Button>
      <Label layoutX="228.0" layoutY="36.0" styleClass="tealtxt" text="Manage Employee Incident" />
      <Button fx:id="employeeIncidentStatusButton" layoutX="150.0" layoutY="191.0" mnemonicParsing="false" onAction="#handleEmployeeIncidentStatusButton" prefHeight="69.0" prefWidth="143.0" text="Employee Incident Status" textAlignment="CENTER" wrapText="true">
         <styleClass>
            <String fx:value="tealbg" />
            <String fx:value="whitetxt" />
         </styleClass></Button>
      <Button fx:id="empEmployeeIncidentButton" layoutX="300.0" layoutY="191.0" mnemonicParsing="false" onAction="#handleEmpEmployeeIncidentButton" prefHeight="69.0" prefWidth="143.0" text="Employee Employee Incident" textAlignment="CENTER" wrapText="true">
         <styleClass>
            <String fx:value="tealbg" />
            <String fx:value="whitetxt" />
         </styleClass></Button>
      <Button fx:id="homeButton" layoutX="507.0" layoutY="341.0" mnemonicParsing="false" onAction="#handleHomeButton" text="Home">
         <styleClass>
            <String fx:value="whitebg" />
            <String fx:value="redtxt" />
         </styleClass></Button>
   </children>
</AnchorPane>
