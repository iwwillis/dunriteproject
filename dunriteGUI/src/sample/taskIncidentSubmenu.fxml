<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.taskIncidentSubmenuController">
   <children>
      <Button fx:id="taskIncidentButton" layoutX="150.0" layoutY="112.0" mnemonicParsing="false" onAction="#handletaskIncidentButton" prefHeight="69.0" prefWidth="143.0" text="Task Incident">
         <styleClass>
            <String fx:value="tealbg" />
            <String fx:value="whitetxt" />
         </styleClass></Button>
      <Button fx:id="backButton" layoutX="452.0" layoutY="341.0" mnemonicParsing="false" onAction="#handleBackButton" text="Back">
         <styleClass>
            <String fx:value="whitebg" />
            <String fx:value="redtxt" />
         </styleClass></Button>
      <Button fx:id="taskIncidentTypeButton" layoutX="300.0" layoutY="112.0" mnemonicParsing="false" onAction="#handletaskIncidentTypeButton" prefHeight="69.0" prefWidth="143.0" text="Task Incident Type">
         <styleClass>
            <String fx:value="tealbg" />
            <String fx:value="whitetxt" />
         </styleClass></Button>
      <Label layoutX="265.0" layoutY="42.0" styleClass="tealtxt" text="Task Incident" />
      <Button fx:id="taskIncidentStatusButton" layoutX="150.0" layoutY="191.0" mnemonicParsing="false" onAction="#handletaskIncidentStatusButton" prefHeight="69.0" prefWidth="143.0" text="Task Incident Status" textAlignment="CENTER" wrapText="true">
         <styleClass>
            <String fx:value="tealbg" />
            <String fx:value="whitetxt" />
         </styleClass></Button>
      <Button fx:id="jobTaskIncidentButton" layoutX="300.0" layoutY="191.0" mnemonicParsing="false" onAction="#handleJobTaskIncidentButton" prefHeight="69.0" prefWidth="143.0" text="Job Task Incident" textAlignment="CENTER" wrapText="true">
         <styleClass>
            <String fx:value="tealbg" />
            <String fx:value="whitetxt" />
         </styleClass></Button>
      <Button fx:id="homeButton" layoutX="507.0" layoutY="341.0" mnemonicParsing="false" onAction="#handleHomeButton" text="Home">
         <styleClass>
            <String fx:value="whitebg" />
            <String fx:value="redtxt" />
         </styleClass></Button>
   </children>
</AnchorPane>
